))))
(if (eq **debug** '*debug*)
    (let* ((opt (optimize-tree comp))
    (gen (generate opt)))
  (print "Compiler:")
  (print comp)
  (print "Optimizer:")
  (print opt)
  (print "Generate:")
  (dolist (x gen) (print x)))
(let* ((gen (assemble (generate (optimize-tree comp))))
       (s (-- (array-size gen))))
        (print s)
	(for i 0 s (print (aref gen i)))
(print 'CONSTB )
(dolist (x *consts*)
	(print x))
(print 'CONSTE )
(print *global-variables-count*)
)))

   (comp-err (e) (print "Compiler error:") (print e)))
